  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "cypress": "^13.10.0",
    "cypress-html-reporter": "^11.0.0",
    "faker": "^6.6.6",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0"
  }
}
