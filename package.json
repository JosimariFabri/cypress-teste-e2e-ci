{
  "name": "exercicio-intercept",
  "version": "1.0.0",
  "description": "cypress intercept",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "report": "npm run mergeJsonReport && npm run generateReport",
    "mergeJsonReport": "npx mochawesome-merge ./mochawesome-report/*.json -o ./mochawesome-report/result.json",
    "generateReport": "npx merge ./mochawesome-report/result.json -o ./mochawesome-report"
  },
